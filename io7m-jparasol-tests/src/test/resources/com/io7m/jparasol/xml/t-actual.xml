<?xml version="1.0" encoding="UTF-8"?>
<g:meta g:version="3" xmlns:g="http://schemas.io7m.com/parasol/glsl-meta">
  <g:program-name>com.io7m.renderer.ForwardLitDirectionalSpecularNormalMapped.fwd_LD_S_N</g:program-name>

  <g:supports>
    <g:version g:number="130" g:api="glsl"/>
    <g:version g:number="100" g:api="glsl-es"/>
    <g:version g:number="400" g:api="glsl"/>
    <g:version g:number="140" g:api="glsl"/>
    <g:version g:number="110" g:api="glsl"/>
    <g:version g:number="410" g:api="glsl"/>
    <g:version g:number="430" g:api="glsl"/>
    <g:version g:number="420" g:api="glsl"/>
    <g:version g:number="330" g:api="glsl"/>
    <g:version g:number="150" g:api="glsl"/>
    <g:version g:number="120" g:api="glsl"/>
    <g:version g:number="300" g:api="glsl-es"/>
  </g:supports>

  <g:declared-vertex-parameters>
    <g:parameter g:name="something" g:type="int"/>
  </g:declared-vertex-parameters>

  <g:declared-fragment-parameters>
    <g:parameter g:name="t_albedo" g:type="sampler2D"/>
    <g:parameter g:name="t_normal" g:type="sampler2D"/>
    <g:parameter g:name="other" g:type="int"/>
  </g:declared-fragment-parameters>

  <g:declared-vertex-inputs>
    <g:input g:name="v_position" g:type="vec4"/>
    <g:input g:name="v_normal" g:type="vec3"/>
  </g:declared-vertex-inputs>

  <g:declared-fragment-inputs>
    <g:input g:name="f_position" g:type="vec4"/>
    <g:input g:name="f_normal" g:type="vec3"/>
    <g:input g:name="f_tangent" g:type="vec3"/>
  </g:declared-fragment-inputs>

  <g:declared-vertex-outputs>
    <g:output g:name="f_position" g:type="vec4"/>
    <g:output g:name="f_normal" g:type="vec3"/>
    <g:output g:name="f_tangent" g:type="vec3"/>
  </g:declared-vertex-outputs>

  <g:declared-fragment-outputs>
    <g:fragment-output g:name="out_0" g:type="vec4" g:index="0"/>
    <g:fragment-output g:name="out_1" g:type="vec4" g:index="1"/>
    <g:fragment-output g:name="out_2" g:type="vec4" g:index="2"/>
  </g:declared-fragment-outputs>

</g:meta>
